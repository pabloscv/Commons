<VirtualHost *:80>
	#ServerName [SITE].com.br
    	#ServerAlias [SITE].com.br
	#ServerAdmin [RESPONSAVEL]@gmail.com

	SSLProxyEngine on
	SSLProxyVerify none 
	SSLProxyCheckPeerCN off
	SSLProxyCheckPeerName off
	SSLProxyCheckPeerExpire off
	
	SSLEngine off
	SSLProtocol all -SSLv3 -TLSv1 -TLSv1.1
    	SSLCipherSuite ALL:!ADH:!EXPORT:!SSLv2:!RC4+RSA:+HIGH:+MEDIUM:!LOW:!RC4

	#SSLCertificateFile /etc/apache2/rsa/certificado.crt
	#SSLCertificateKeyFile /etc/apache2/rsa/chave.key
	#SSLCACertificateFile /etc/apache2/rsa/certificado_ca.ca-bundle
	
	RequestHeader set X-Forwarded-Proto https
	RequestHeader set X-Forwarded-Port 443
	Header always set Strict-Transport-Security "max-age=31536000; includeSubDomains"

        Protocols h2 h2c http/1.1
	H2Push on
	H2PushPriority * after
	H2PushPriority text/css before
	H2PushPriority image/jpg after 32
	H2PushPriority image/jpeg after 32
	H2PushPriority image/png after 32
	H2PushPriority application/javascript interleaved

	ProxyPreserveHost On
	ProxyRequests Off

	ProxyPass /emailtest/ https://127.0.0.1:8299/
	ProxyPassReverse /emailtest/ https://127.0.0.1:8299/

	ProxyPass /settwebi/ https://127.0.0.1:8230/
	ProxyPassReverse /settwebi/ https://127.0.0.1:8230/

	ProxyPass /authorization/ https://127.0.0.1:8221/
	ProxyPassReverse /authorization/ https://127.0.0.1:8221/

	ProxyPass / https://127.0.0.1:8400/
	ProxyPassReverse / https://127.0.0.1:8400/

	ErrorLog ${APACHE_LOG_DIR}/error.log
	CustomLog ${APACHE_LOG_DIR}/access.log combined
</VirtualHost>


